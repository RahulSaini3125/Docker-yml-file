# .env.example
# Copy this file to `.env` and set secure values before running the compose stack.
# Do NOT commit a real `.env` with secrets.

# MySQL
# Root password for MySQL. Example: a strong password, not "admin@123".
MYSQL_PASSWORD=your_mysql_root_password_here
# MySQL image tag (optional). Default: latest
MYSQL_VERSION=latest

# PostgreSQL
# PostgreSQL image tag (optional). Default: latest
POSTGRES_VERSION=latest
# Password for the default `postgres` user
POSTGRES_PASSWORD=your_postgres_password_here

# MongoDB
# MongoDB init root user and password
MONGO_USERNAME=your_mongo_root_username_here
MONGO_PASSWORD=your_mongo_root_password_here
# MongoDB image tag (optional). Default: latest
MONGODB_VERSION=latest
